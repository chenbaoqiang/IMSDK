<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:orientation="vertical"
              android:layout_width="match_parent"
              android:layout_height="match_parent">


    <Button android:layout_width="wrap_content" android:layout_height="wrap_content"
            android:id="@+id/btn_buddy" android:text="好友" />

    <Button android:layout_width="wrap_content" android:layout_height="wrap_content"
            android:id="@+id/btn_buddy_ntf" android:text="好友通知" />

    <Button android:layout_width="wrap_content" android:layout_height="wrap_content"
            android:id="@+id/btn_user" android:text="个人资料" />

    <Button android:layout_width="wrap_content" android:layout_height="wrap_content"
            android:id="@+id/btn_setting_profile" android:text="设置个人资料" />

    <Button android:layout_width="wrap_content" android:layout_height="wrap_content"
            android:id="@+id/btn_fetch_file" android:text="下载文件" />

    <Button android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/btn_group_list" android:text="群组列表" />

    <Button android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/btn_group_evt" android:text="群组事件" />


    <Button android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/btn_custom_message" android:text="自定义消息" />
    <Button android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/btn_presence" android:text="订阅好友状态"/>
    <Button android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/btn_device" android:text="设备"/>


</LinearLayout>
